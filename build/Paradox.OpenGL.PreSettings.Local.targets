<!-- Build file pre-included by all Paradox projects -->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <SiliconStudioCommonPreSettingsName>Paradox</SiliconStudioCommonPreSettingsName>

    <!--  Defines the API used by Paradox - Values are Direct3D/Null/OpenGL/OpenGLES -->
    <ParadoxGraphicsApi Condition=" '$(ParadoxGraphicsApi)' == '' ">OpenGL</ParadoxGraphicsApi>
    <SiliconStudioParadoxGraphicsApi>$(ParadoxGraphicsApi)</SiliconStudioParadoxGraphicsApi>

    <!-- Defines the platform used by Paradox - Values are Windows/WindowsStore/WindowsPhone/Android/iOS...etc. -->
    <SiliconStudioPlatform Condition=" '$(SiliconStudioPlatform)' == '' ">Windows</SiliconStudioPlatform>

    <!-- Defines target for Asset compiler TODO: this is hardcoded -->
    <ParadoxProjectBuildProfile>Windows_OGL</ParadoxProjectBuildProfile>
    
    <SiliconStudioPlatformForceMono>true</SiliconStudioPlatformForceMono>
  </PropertyGroup>
</Project>