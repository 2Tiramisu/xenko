class ComputeColorMaterialAlphaBlend<bool TDiffuse> : ComputeColor
{
    compose ComputeColor color;

    override float4 Compute()
    {
        var value = color.Compute();
        if (TDiffuse)
        {
            return float4(value.x, value.x, 0, 0);
        }

        return float4(0, value.x, 0, 0);
    }
};
