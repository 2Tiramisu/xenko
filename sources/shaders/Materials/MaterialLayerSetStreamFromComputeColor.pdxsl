namespace SiliconStudio.Paradox.Effects.Materials
{
    class MaterialLayerSetStreamFromComputeColor<MemberName TStream, MemberName TChannel> : IMaterialLayer
    {
        compose ComputeColor computeColorSource;

        override void Compute()
        {
            streams.TStream = computeColorSource.Compute().TChannel;
        }
    };
}