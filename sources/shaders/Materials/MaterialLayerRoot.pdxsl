namespace SiliconStudio.Paradox.Effects.Materials
{
    class MaterialLayerRoot : ShaderBase, MaterialStreams
    {
        compose IMaterialLayer composition;

        override void PSMain()
        {
            composition.Compute();
            streams.ColorTarget = float4(streams.matNormal, 1);
        }
    };
}