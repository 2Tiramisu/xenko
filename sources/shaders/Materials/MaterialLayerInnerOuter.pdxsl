class MaterialLayerInnerOuter : IMaterialLayer
{
    compose IMaterialLayer innerLayer;
    compose IMaterialLayer innerToOuterlayer;

    override void Compute(Streams input1Stream, Streams input2Stream, Streams outputStream)
    {
        Streams innerStreams = 0;
        innerLayer.Compute(innerStreams, innerStreams, innerStreams);
        innerToOuterlayer.Compute(input1Stream, innerStreams, outputStream);
    }
};