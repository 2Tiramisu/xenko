/// <summary>
/// Computes the transformation matrix from tangent to view space when skinning occured.
/// </summary>
class NormalVSSkinningNormalMapping : NormalVSFromNormalMapping
{
    override float3x3 GetTangentViewTransform()
    {
        return (float3x3)View;
    }
};